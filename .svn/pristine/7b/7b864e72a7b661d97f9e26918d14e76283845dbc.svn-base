function searchByAgent() {
    var fname_field = document.getElementById('name');
    var fname = document.getElementById('name').value;
    var lname_field = document.getElementById('lname');
    var lname = document.getElementById('lname').value;
    var mob_field = document.getElementById('mobilenumber');
    var mob_num = document.getElementById('mobilenumber').value;
    var err_field = document.getElementById('ag_error');

    if (mob_num == '' && fname == '' && lname == '') {
        err_field.innerHTML = '<div class="error"><img src="/images/cmt_error.gif" width="11" height="11" alt="" /> Error! Please, fill at least one field to do search.</div>';
        err_field.parentNode.style.display = 'table-row';
        return false;
    }

    if (!(/^[-+0-9()]*$/.test(mob_num))) {
        err_field.innerHTML='<div class="error"><img src="/images/cmt_error.gif" width="11" height="11" alt="" /> Error! The field contains Invalid characters. Please check and try again. Error # 1005</div>';
        err_field.parentNode.style.display = 'table-row';
        mob_field.style.border = '1px solid #ff0000';
        return false;
    }
    else {
        err_field.parentNode.style.display = 'none';
        mob_field.style.border = '1px solid #ccc';
    }

    if (!(/^[0-9a-zA-Z]*$/.test(fname))) {
        err_field.innerHTML='<div class="error"><img src="/images/cmt_error.gif" width="11" height="11" alt="" /> Error! The field contains Invalid characters. Please check and try again. Error # 1005</div>';
        err_field.parentNode.style.display = 'table-row';
        fname_field.style.border = '1px solid #ff0000';
        return false;
    }
    else {
        err_field.parentNode.style.display = 'none';
        fname_field.style.border = '1px solid #ccc';
    }

    if (!(/^[0-9a-zA-Z]*$/.test(lname))) {
        err_field.innerHTML='<div class="error"><img src="/images/cmt_error.gif" width="11" height="11" alt="" /> Error! The field contains Invalid characters. Please check and try again. Error # 1005</div>';
        err_field.parentNode.style.display = 'table-row';
        lname_field.style.border = '1px solid #ff0000';
        return false;
    }
    else {
        err_field.parentNode.style.display = 'none';
        lname_field.style.border = '1px solid #ccc';
    }

    sndReqSearchByAgent(
        0,
        document.getElementById('mobilenumber').value,
        document.getElementById('name').value,
        document.getElementById('lname').value,
        '',
        'Agent',
        '',
        '',
        '',
        '',
        ''
    );
}
